{
  "family": "cicd-demo",
  "containerDefinitions": [
    {
      "name": "cicd-demo",
      "image": "cicd-demo:latest",
      "entryPoint": ["sh", "-c"],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "memory": "500",
  "requiresCompatibilities": ["EC2"]
}
