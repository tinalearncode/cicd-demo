{
  "family": "cicd-demo",
  "containerDefinitions": [
    {
      "name": "cicd-demo",
      "image": "823955147616.dkr.ecr.ap-southeast-2.amazonaws.com/cicd-demo:latest",
      "entryPoint": ["sh", "-c"],
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "cpu": 10,
      "memory": 300
    }
  ],
  "compatibilities": ["EC2", "EXTERNAL"],
  "requiresCompatibilities": ["EC2"]
}
