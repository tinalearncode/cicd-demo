{
  "family": "cicd-demo",
  "containerDefinitions": [
    {
      "name": "cicd-demo-container",
      "image": "cicd-demo-container/cicd-demo-container:latest",
      "entryPoint": ["sh", "-c"],
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "cpu": 10,
      "memory": 300
    }
  ],
  "compatibilities": ["EC2", "EXTERNAL"],
  "requiresCompatibilities": ["EC2"]
}
